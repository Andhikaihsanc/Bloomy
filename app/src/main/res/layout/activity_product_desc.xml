<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="productDescVM" type="com.blommy.app.modules.productdesc.data.viewmodel.ProductDescVM"/>
    </data>
    <FrameLayout android:id="@+id/frameProductdesc"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_100">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_28pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <FrameLayout android:id="@+id/frameStackvector"
                        android:layout_height="@dimen/_18pxv"
                        android:layout_marginStart="@dimen/_262pxh"
                        android:layout_marginTop="@dimen/_7pxv" android:layout_width="@dimen/_21pxh">
                        <ImageView android:id="@+id/imageVector"
                            android:layout_gravity="bottom|start"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGroupEleven"
                            android:layout_gravity="top|end"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_2pxv"
                            android:paddingEnd="@dimen/_2pxh"
                            android:paddingStart="@dimen/_2pxh"
                            android:paddingTop="@dimen/_1pxv"
                            android:singleLine="true"
                            android:text="@{productDescVM.productDescModel.txtGroupEleven}"
                            style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_2"/>
                    </FrameLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <FrameLayout android:id="@+id/frameStackrectanglesixtyfive"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_267pxv"
                android:layout_marginEnd="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_20pxv" android:layout_width="@dimen/_297pxh">
                <ImageView android:id="@+id/imageRectangleSixtyFive"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_267pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_5pxh}"
                    imageRes="@{@drawable/img_ellipse2_34x34}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse2_34x34"/>
                <ImageView android:id="@+id/imageGroup106"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_5pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_128pxh"
                    android:layout_marginStart="@dimen/_128pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_39pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group106"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group106"/>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearRowbouquetmawar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBouquetMawar"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.85"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{productDescVM.productDescModel.txtBouquetMawar}"
                    style="@style/txtOutfitmedium31" tools:text="@string/lbl_bouquet_mawar2"/>
                <ImageButton
                    android:background="@drawable/rectangle_bg_white_a700_radius_5"
                    android:id="@+id/btnGroupTen"
                    android:layout_height="@dimen/_38pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_43pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_38pxh"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_group10" tools:ignore="ContentDescription"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtRpCounter"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{productDescVM.productDescModel.txtRpCounter}"
                style="@style/txtOutfitmedium20_2" tools:text="@string/lbl_rp_124_000"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowvectorOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVectorOne"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_orange_300"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_orange_300"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFortyEight"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_weight="0.22"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{productDescVM.productDescModel.txtFortyEight}"
                    style="@style/txtOutfitlight16" tools:text="@string/lbl_4_82"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txt30reviews"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.64"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{productDescVM.productDescModel.txt30reviews}"
                    style="@style/txtOutfitextralight12" tools:text="@string/lbl_30_reviews"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_45pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{productDescVM.productDescModel.txtDescription}"
                style="@style/txtOutfitmedium16_1" tools:text="@string/lbl_description"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtDescriptionOne"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent"
                android:text="@{productDescVM.productDescModel.txtDescriptionOne}"
                style="@style/txtOutfitlight12" tools:text="@string/msg_duis_aute_irure"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowrectanglesixteen"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_32pxh"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameAddbutton"
                    android:layout_height="@dimen/_39pxv"
                    android:layout_marginBottom="@dimen/_1pxv" android:layout_width="@dimen/_35pxh">
                    <View
                        android:background="@drawable/rectangle_bg_white_a700_border_black_900_4c"
                        android:id="@+id/viewRectangleSixteen"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_35pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_35pxh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtOne"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{productDescVM.productDescModel.txtOne}"
                        style="@style/txtInterlight32" tools:text="@string/lbl3"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameQuantity"
                    android:layout_height="@dimen/_26pxv"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_14pxh">
                    <View android:background="@color/white_A700"
                        android:id="@+id/viewRectangleSixteenOne"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh" android:layout_width="@dimen/_14pxh"/>
                    <TextView android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtTwo"
                        android:layout_gravity="bottom|end"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{productDescVM.productDescModel.txtTwo}"
                        style="@style/txtInterregular20" tools:text="@string/lbl_2"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameAddbutton1"
                    android:layout_height="@dimen/_39pxv"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_35pxh" style="@style/groupStylewhite_4">
                    <View android:background="@color/white_A700_33"
                        android:id="@+id/viewRectangleSixteenTwo"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_35pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_2pxv" android:layout_width="@dimen/_35pxh"/>
                    <TextView android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtThree"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_8pxh"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{productDescVM.productDescModel.txtThree}"
                        style="@style/txtInterlight32" tools:text="@string/lbl4"/>
                </FrameLayout>
            </LinearLayout>
            <View android:background="@color/black_900_4c"
                android:id="@+id/lineLineFive"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_32pxh"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_4pxv" android:layout_width="@dimen/_40pxh"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnAddToCart"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_32pxh"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_10pxv"
                android:singleLine="true"
                android:text="@string/lbl_add_to_cart"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_add_to_cart"/>
        </LinearLayout>
    </FrameLayout>
</layout>
