<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="tambahBarangSellerVM" type="com.blommy.app.modules.tambahbarangseller.data.viewmodel.TambahBarangSellerVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_29pxh"
        android:layout_marginStart="@dimen/_26pxh"
        android:layout_marginTop="@dimen/_45pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_100">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_2pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTambahBarang"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_65pxh"
                    android:layout_weight="0.9"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{tambahBarangSellerVM.tambahBarangSellerModel.txtTambahBarang}"
                    style="@style/txtOutfitmedium20" tools:text="@string/lbl_tambah_barang"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnvector"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:id="@+id/frameStackvector"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_134pxv"
                    android:layout_marginEnd="@dimen/_85pxh"
                    android:layout_marginStart="@dimen/_85pxh"
                    android:layout_width="@dimen/_134pxh" style="@style/groupStylered_301">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_gravity="top|center"
                        android:layout_height="@dimen/_53pxh"
                        android:layout_marginBottom="@dimen/_33pxv"
                        android:layout_marginEnd="@dimen/_34pxh"
                        android:layout_marginStart="@dimen/_34pxh"
                        android:layout_marginTop="@dimen/_33pxv"
                        android:layout_width="@dimen/_62pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_gray_200"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_200"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNamaBarang"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.08"
                    android:singleLine="true"
                    android:text="@{tambahBarangSellerVM.tambahBarangSellerModel.txtNamaBarang}"
                    style="@style/txtOutfitsemibold16" tools:text="@string/lbl_nama_barang"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_red_300"
                    android:ellipsize="end"
                    android:hint="@string/lbl_nama_barang2"
                    android:id="@+id/etGroupSeven"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.08"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_8pxv"
                    android:singleLine="true"
                    android:text="@={tambahBarangSellerVM.tambahBarangSellerModel.etGroupSevenValue}"
                    android:textColorHint="@color/black_900_7f"
                    style="@style/etSolidOutline" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.08"
                    android:singleLine="true"
                    android:text="@{tambahBarangSellerVM.tambahBarangSellerModel.txtLanguage}"
                    style="@style/txtOutfitsemibold16" tools:text="@string/lbl_deskripsi"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_red_300"
                    android:ellipsize="end"
                    android:hint="@string/lbl_deskripsi2"
                    android:id="@+id/etGroupSix"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.08"
                    android:paddingBottom="@dimen/_30pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_8pxv"
                    android:singleLine="true"
                    android:text="@={tambahBarangSellerVM.tambahBarangSellerModel.etGroupSixValue}"
                    android:textColorHint="@color/black_900_7f"
                    style="@style/etSolidOutline" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBanyakbarang"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.08"
                    android:singleLine="true"
                    android:text="@{tambahBarangSellerVM.tambahBarangSellerModel.txtBanyakbarang}"
                    style="@style/txtOutfitsemibold16" tools:text="@string/lbl_banyak_barang"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_red_300"
                    android:ellipsize="end"
                    android:hint="@string/lbl_banyak_barang"
                    android:id="@+id/etGroupFive"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.08"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_8pxv"
                    android:singleLine="true"
                    android:text="@={tambahBarangSellerVM.tambahBarangSellerModel.etGroupFiveValue}"
                    android:textColorHint="@color/black_900_7f"
                    style="@style/etSolidOutline" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguageOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.08"
                    android:singleLine="true"
                    android:text="@{tambahBarangSellerVM.tambahBarangSellerModel.txtLanguageOne}"
                    style="@style/txtOutfitsemibold16" tools:text="@string/lbl_harga"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_red_300"
                    android:ellipsize="end"
                    android:hint="@string/lbl_rp_0"
                    android:id="@+id/etRpCounter"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.08"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_8pxv"
                    android:singleLine="true"
                    android:text="@={tambahBarangSellerVM.tambahBarangSellerModel.etRpCounterValue}"
                    android:textColorHint="@color/black_900_7f"
                    style="@style/etSolidOutline" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguageTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.08"
                    android:singleLine="true"
                    android:text="@{tambahBarangSellerVM.tambahBarangSellerModel.txtLanguageTwo}"
                    style="@style/txtOutfitsemibold16" tools:text="@string/lbl_kategori2"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_red_300"
                    android:ellipsize="end"
                    android:hint="@string/lbl_kategori3"
                    android:id="@+id/etGroupFour"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.08"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:text="@={tambahBarangSellerVM.tambahBarangSellerModel.etGroupFourValue}"
                    android:textColorHint="@color/black_900_7f"
                    style="@style/etSolidOutline" tools:ignore="Autofill"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSubmit"
                    android:layout_height="@dimen/_33pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="@dimen/_79pxh"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_submit"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_3" tools:text="@string/lbl_submit"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnkatonabeatrix"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_93pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylebluegray_100">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtKatonaBeatrix"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.08"
                        android:singleLine="true"
                        android:text="@{tambahBarangSellerVM.tambahBarangSellerModel.txtKatonaBeatrix}"
                        style="@style/txtOutfitsemibold16" tools:text="@string/lbl_katona_beatrix"/>
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtUtenimadmini"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.06"
                        android:text="@{tambahBarangSellerVM.tambahBarangSellerModel.txtUtenimadmini}"
                        style="@style/txtOutfitlight12_2" tools:text="@string/msg_ut_enim_ad_mini"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRow20200505102"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_29pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt20200505102"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_weight="0.68"
                            android:layout_width="0dp"
                            android:letterSpacing="0.06"
                            android:singleLine="true"
                            android:text="@{tambahBarangSellerVM.tambahBarangSellerModel.txt20200505102}"
                            style="@style/txtOutfitlight12_3" tools:text="@string/msg_2020_05_05_10_2"/>
                        <ImageView android:id="@+id/imageVectorOne"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginStart="@dimen/_42pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_orange_300"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_orange_300"/>
                        <ImageView android:id="@+id/imageVectorTwo"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_orange_300"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_orange_300"/>
                        <ImageView android:id="@+id/imageVectorThree"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_orange_300"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_orange_300"/>
                        <ImageView android:id="@+id/imageVectorFour"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_orange_300"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_orange_300"/>
                        <ImageView android:id="@+id/imageVectorFive"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_gray_400"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_400"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
