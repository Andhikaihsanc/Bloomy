<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listmoreverticalRowModel" type="com.blommy.app.modules.checkout.data.model.ListmoreverticalRowModel"/>
    </data>
    <LinearLayout android:gravity="end"
        android:id="@+id/linearColumnmorevertical"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical" style="@style/groupStylewhite_A700_8ecornerRadius">
        <ImageView android:id="@+id/imageMorevertical"
            android:layout_height="@dimen/_11pxh"
            android:layout_marginEnd="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="@dimen/_2pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_morevertical"
            tools:ignore="ContentDescription" tools:src="@drawable/img_morevertical"/>
        <LinearLayout android:id="@+id/linearRowjlhrrasuna"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_13pxv"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_6pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnjlhrrasuna"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_weight="0.93" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtJlHRRasuna"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{listmoreverticalRowModel.txtJlHRRasuna}"
                    style="@style/txtOutfitlight13_1" tools:text="@string/msg_jl_h_r_rasuna"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtZipcode"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{listmoreverticalRowModel.txtZipcode}"
                    style="@style/txtOutfitlight13_1" tools:text="@string/lbl_87201"/>
                <FrameLayout android:id="@+id/frameStackkotamalang"
                    android:layout_height="@dimen/_31pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_74pxh">
                    <TextView android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtKotaMalang"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{listmoreverticalRowModel.txtKotaMalang}"
                        style="@style/txtOutfitlight13_1" tools:text="@string/lbl_kota_malang"/>
                    <TextView android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtCountry"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{listmoreverticalRowModel.txtCountry}"
                        style="@style/txtOutfitlight13_1" tools:text="@string/lbl_indonesia"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDeliveryAdress"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{listmoreverticalRowModel.txtDeliveryAdress}"
                    style="@style/txtOutfitlight10_3" tools:text="@string/lbl_delivery_adress"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageVector"
                android:layout_height="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_68pxv"
                android:layout_width="@dimen/_15pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_red_300_15x15"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_red_300_15x15"/>
        </LinearLayout>
    </LinearLayout>
</layout>
