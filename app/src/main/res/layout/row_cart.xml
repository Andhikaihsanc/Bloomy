<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="cartRowModel" type="com.blommy.app.modules.cart.data.model.CartRowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameStackellipseone"
        android:layout_height="wrap_content" android:layout_width="match_parent">
        <LinearLayout android:id="@+id/linearRowellipseone"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_A700_8ecornerRadius">
            <ImageView android:id="@+id/imageEllipseOne"
                android:layout_height="@dimen/_44pxh"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="@dimen/_45pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_ellipse1_44x45}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse1_44x45"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnbouquetmawar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_weight="0.71" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBouquetMawar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{cartRowModel.txtBouquetMawar}"
                    style="@style/txtOutfitmedium20_1" tools:text="@string/lbl_bouquet_mawar2"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{cartRowModel.txtLanguage}"
                    style="@style/txtOutfitlight13" tools:text="@string/lbl_lorem_ipsum"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtRpCounter"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{cartRowModel.txtRpCounter}"
                    style="@style/txtOutfitmedium20_1" tools:text="@string/lbl_rp_124_000"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnrectanglesixteen"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_5pxv"
                android:layout_marginEnd="@dimen/_31pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_53pxv"
                android:layout_weight="0.09" android:layout_width="0dp" android:orientation="vertical">
                <FrameLayout android:id="@+id/frameQuantity"
                    android:layout_height="@dimen/_19pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh" android:layout_width="@dimen/_16pxh">
                    <View android:background="@color/white_A700"
                        android:id="@+id/viewRectangleSixteen"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_16pxh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTwo"
                        android:layout_gravity="bottom|end"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{cartRowModel.txtTwo}"
                        style="@style/txtInterregular14" tools:text="@string/lbl_2"/>
                </FrameLayout>
                <View android:background="@color/black_900_4c"
                    android:id="@+id/lineLineThree"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_21pxh"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameAddbutton1"
            android:layout_gravity="bottom|end"
            android:layout_height="@dimen/_39pxv"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_56pxh"
            android:layout_marginStart="@dimen/_56pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="@dimen/_20pxh" style="@style/groupStylecornerRadius_1">
            <View
                android:background="@drawable/rectangle_bg_white_a700_border_black_900_4c"
                android:id="@+id/viewRectangleSixteenOne"
                android:layout_gravity="start"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_9pxv"
                android:layout_marginTop="@dimen/_9pxv" android:layout_width="@dimen/_20pxh"/>
            <TextView android:ellipsize="end"
                android:gravity="top|start" android:id="@+id/txtFour"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{cartRowModel.txtFour}"
                style="@style/txtInterlight32" tools:text="@string/lbl3"/>
        </FrameLayout>
        <FrameLayout android:id="@+id/frameAddbutton2"
            android:layout_gravity="bottom|end"
            android:layout_height="@dimen/_39pxv"
            android:layout_marginEnd="@dimen/_7pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="@dimen/_20pxh" style="@style/groupStylewhite_4">
            <View android:background="@color/white_A700_33"
                android:id="@+id/viewRectangleSixteenTwo"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_20pxh"/>
            <TextView android:ellipsize="end"
                android:gravity="top|start" android:id="@+id/txtFive"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{cartRowModel.txtFive}"
                style="@style/txtInterlight32" tools:text="@string/lbl4"/>
        </FrameLayout>
    </FrameLayout>
</layout>
