<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="checkoutVM" type="com.blommy.app.modules.checkout.data.viewmodel.CheckoutVM"/>
    </data>
    <FrameLayout android:id="@+id/frameCheckout"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_100">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="end"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCheckout"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_94pxh"
                        android:layout_weight="0.84"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{checkoutVM.checkoutModel.txtCheckout}"
                        style="@style/txtOutfitmedium20" tools:text="@string/lbl_checkout"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDeliveryAddres"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{checkoutVM.checkoutModel.txtDeliveryAddres}"
                style="@style/txtOutfitregular16_2" tools:text="@string/msg_delivery_addres"/>
            <LinearLayout android:id="@+id/linearRowgroup"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackgroup"
                    android:layout_height="@dimen/_120pxv"
                    android:layout_width="@dimen/_38pxh" style="@style/groupStylewhite_A700_8ecornerRadius">
                    <ImageView android:id="@+id/imageGroup"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_40pxv"
                        android:layout_marginEnd="@dimen/_9pxh"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_40pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group"/>
                </FrameLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerListmorevertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_weight="0.89"
                    android:layout_width="0dp"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_11pxh}" tools:itemCount="2" tools:listitem="@layout/row_listmorevertical"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtOrderSummary"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{checkoutVM.checkoutModel.txtOrderSummary}"
                style="@style/txtOutfitregular16_2" tools:text="@string/lbl_order_summary"/>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerListellipseone"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_6pxh}" tools:itemCount="2" tools:listitem="@layout/row_listellipseone"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguageTwo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{checkoutVM.checkoutModel.txtLanguageTwo}"
                style="@style/txtOutfitmedium13" tools:text="@string/lbl_atur_pengiriman"/>
            <FrameLayout android:id="@+id/frameStacktime"
                android:layout_height="@dimen/_68pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_2pxv" android:layout_width="@dimen/_293pxh">
                <LinearLayout android:id="@+id/linearColumntime"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylered_301">
                    <Spinner android:background="@color/gray_101"
                        android:gravity="center_horizontal"
                        android:id="@+id/spinnerTime"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_19pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowgroupeightysix"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylered_301">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGroupEightySix"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_1pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_2pxv"
                        android:singleLine="true"
                        android:text="@{checkoutVM.checkoutModel.txtGroupEightySix}"
                        style="@style/txtSolidRoundedLeftCorner_2" tools:text="@string/lbl_delivery"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGroupEightyFive"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_1pxv"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_2pxv"
                        android:singleLine="true"
                        android:text="@{checkoutVM.checkoutModel.txtGroupEightyFive}"
                        style="@style/txtSolidRoundedRightCorner" tools:text="@string/lbl_self_pick_up"/>
                </LinearLayout>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameStackpilihpembayara"
                android:layout_height="@dimen/_77pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_293pxh">
                <LinearLayout
                    android:id="@+id/linearColumnpilihpembayara"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylered_2">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtPilihPembayara"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{checkoutVM.checkoutModel.txtPilihPembayara}"
                        style="@style/txtOutfitregular15_1" tools:text="@string/msg_pilih_pembayara"/>
                </LinearLayout>
                <View
                    android:background="@drawable/rectangle_bg_white_a700_border_gray_900_60"
                    android:id="@+id/viewRectangleSixtyTwo"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_55pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_275pxh"/>
                <LinearLayout android:id="@+id/linearRowgroupeightynine"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_7">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGroupEightyNine"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_17pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_1pxv"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_1pxv"
                        android:singleLine="true"
                        android:text="@{checkoutVM.checkoutModel.txtGroupEightyNine}"
                        style="@style/txtSolidRoundedLeftCorner_2" tools:text="@string/lbl_cod"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGroupEightyEight"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_17pxv"
                        android:layout_marginEnd="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_1pxv"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_1pxv"
                        android:singleLine="true"
                        android:text="@{checkoutVM.checkoutModel.txtGroupEightyEight}"
                        style="@style/txtSolidRoundedRightCorner" tools:text="@string/lbl_m_banking"/>
                </LinearLayout>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameStacklineone"
                android:layout_height="@dimen/_96pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="@dimen/_292pxh" style="@style/groupStyle">
                <LinearLayout android:id="@+id/linearColumnlineone"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <View android:background="@color/red_300"
                        android:id="@+id/lineLineOne"
                        android:layout_height="@dimen/_3pxv" android:layout_width="@dimen/_290pxh"/>
                    <View android:background="@color/red_300"
                        android:id="@+id/lineLineTwo"
                        android:layout_height="@dimen/_3pxv" android:layout_width="@dimen/_290pxh"/>
                </LinearLayout>
                <View android:background="@color/red_300"
                    android:id="@+id/lineLineThree"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_3pxv"
                    android:layout_marginBottom="@dimen/_32pxv"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_32pxv" android:layout_width="@dimen/_290pxh"/>
                <LinearLayout android:id="@+id/linearRowhargabarang"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnhargabarang"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_weight="0.78"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHargaBarang"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{checkoutVM.checkoutModel.txtHargaBarang}"
                            style="@style/txtOutfitregular13" tools:text="@string/lbl_harga_barang"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtOngkir"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{checkoutVM.checkoutModel.txtOngkir}"
                            style="@style/txtOutfitregular13" tools:text="@string/lbl_ongkir"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLanguageThree"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{checkoutVM.checkoutModel.txtLanguageThree}"
                            style="@style/txtOutfitmedium13" tools:text="@string/lbl_total"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnrpcounterTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_weight="0.22"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtRpCounterTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_1pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{checkoutVM.checkoutModel.txtRpCounterTwo}"
                            style="@style/txtOutfitregular13" tools:text="@string/lbl_rp_243_123"/>
                        <TextView android:ellipsize="end"
                            android:gravity="end"
                            android:id="@+id/txtLanguageFour"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_1pxh"
                            android:layout_marginStart="@dimen/_7pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{checkoutVM.checkoutModel.txtLanguageFour}"
                            style="@style/txtOutfitregular13" tools:text="@string/lbl_rp_3_000"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLanguageFive"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{checkoutVM.checkoutModel.txtLanguageFive}"
                            style="@style/txtOutfitmedium13" tools:text="@string/lbl_rp_246_123"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnCheckoutOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_86pxh"
                android:layout_marginStart="@dimen/_86pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_8pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_8pxv"
                android:singleLine="true"
                android:text="@string/lbl_checkout2"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_4" textUnderline="true" tools:text="@string/lbl_checkout2"/>
        </LinearLayout>
    </FrameLayout>
</layout>
