<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="personalReviewsOneVM" type="com.blommy.app.modules.personalreviewsone.data.viewmodel.PersonalReviewsOneVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_28pxh"
        android:layout_marginStart="@dimen/_28pxh"
        android:layout_marginTop="@dimen/_44pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_100">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtReviews"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_100pxh"
                    android:layout_weight="0.82"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{personalReviewsOneVM.personalReviewsOneModel.txtReviews}"
                    style="@style/txtOutfitmedium20" tools:text="@string/lbl_reviews"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_39pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumngroup132"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageGroup132"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginEnd="@dimen/_82pxh"
                    android:layout_marginStart="@dimen/_83pxh"
                    android:layout_width="@dimen/_138pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group132"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group132"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.08"
                    android:singleLine="true"
                    android:text="@{personalReviewsOneVM.personalReviewsOneModel.txtLanguage}"
                    style="@style/txtOutfitsemibold16" tools:text="@string/lbl_tambah_ulasan"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumntuliskanulasan"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_5">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtTuliskanulasan"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_85pxv"
                        android:layout_marginEnd="@dimen/_14pxh"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.08"
                        android:singleLine="true"
                        android:text="@{personalReviewsOneVM.personalReviewsOneModel.txtTuliskanulasan}"
                        style="@style/txtOutfitthin16" tools:text="@string/msg_tuliskan_ulasan"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowperlihatkannam"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPerlihatkannam"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:letterSpacing="0.06"
                        android:singleLine="true"
                        android:text="@{personalReviewsOneVM.personalReviewsOneModel.txtPerlihatkannam}"
                        style="@style/txtOutfitlight12_2" tools:text="@string/msg_perlihatkan_nam"/>
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_128pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_27pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_red_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_red_300"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSubmit"
                    android:layout_height="@dimen/_33pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="@dimen/_79pxh"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_submit"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_3" tools:text="@string/lbl_submit"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnkatonabeatrix"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_416pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylebluegray_100">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtKatonaBeatrix"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.08"
                        android:singleLine="true"
                        android:text="@{personalReviewsOneVM.personalReviewsOneModel.txtKatonaBeatrix}"
                        style="@style/txtOutfitsemibold16" tools:text="@string/lbl_katona_beatrix"/>
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtUtenimadmini"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.06"
                        android:text="@{personalReviewsOneVM.personalReviewsOneModel.txtUtenimadmini}"
                        style="@style/txtOutfitlight12_2" tools:text="@string/msg_ut_enim_ad_mini"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRow20200505102"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_29pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt20200505102"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_weight="0.68"
                            android:layout_width="0dp"
                            android:letterSpacing="0.06"
                            android:singleLine="true"
                            android:text="@{personalReviewsOneVM.personalReviewsOneModel.txt20200505102}"
                            style="@style/txtOutfitlight12_3" tools:text="@string/msg_2020_05_05_10_2"/>
                        <ImageView android:id="@+id/imageVectorOne"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginStart="@dimen/_42pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_orange_300"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_orange_300"/>
                        <ImageView android:id="@+id/imageVectorTwo"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_orange_300"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_orange_300"/>
                        <ImageView android:id="@+id/imageVectorThree"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_orange_300"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_orange_300"/>
                        <ImageView android:id="@+id/imageVectorFour"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_orange_300"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_orange_300"/>
                        <ImageView android:id="@+id/imageVectorFive"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_gray_400"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_400"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
