<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listkatonabeatrixRowModel" type="com.blommy.app.modules.reviews.data.model.ListkatonabeatrixRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowkatonabeatrix"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylewhite_5">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnkatonabeatrix"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_weight="0.95" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtKatonaBeatrix"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent"
                android:letterSpacing="0.08" android:singleLine="true"
                android:text="@{listkatonabeatrixRowModel.txtKatonaBeatrix}"
                style="@style/txtOutfitsemibold16" tools:text="@string/lbl_katona_beatrix"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtUtenimadmini"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="match_parent"
                android:letterSpacing="0.06"
                android:text="@{listkatonabeatrixRowModel.txtUtenimadmini}"
                style="@style/txtOutfitlight12_2" tools:text="@string/msg_ut_enim_ad_mini"/>
            <LinearLayout android:id="@+id/linearRow20200505102"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txt20200505102"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.68"
                    android:layout_width="0dp"
                    android:letterSpacing="0.06"
                    android:singleLine="true"
                    android:text="@{listkatonabeatrixRowModel.txt20200505102}"
                    style="@style/txtOutfitlight12_3" tools:text="@string/msg_2020_05_05_10_2"/>
                <LinearLayout android:id="@+id/linearRowvector"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_66pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.32"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVectorFive"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_orange_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_orange_300"/>
                    <ImageView android:id="@+id/imageVectorOneFive"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_orange_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_orange_300"/>
                    <ImageView android:id="@+id/imageVectorTwoFive"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_orange_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_orange_300"/>
                    <ImageView android:id="@+id/imageVectorThreeFive"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_orange_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_orange_300"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageVectorFourFive"
            android:layout_height="@dimen/_14pxh"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_marginEnd="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_99pxv"
            android:layout_width="@dimen/_14pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_vector_gray_400"
            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_400"/>
    </LinearLayout>
</layout>
